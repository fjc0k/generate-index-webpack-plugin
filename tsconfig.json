{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "declaration": true,
    "removeComments": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "preserveSymlinks": true,
    "incremental": true,
    "tsBuildInfoFile": "./node_modules/.cache/tsbif",
    "newLine": "LF",
    "jsx": "react",
    "lib": [
      "esnext",
      "dom"
    ]
  }
}